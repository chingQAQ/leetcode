{
  "eslint.validate": [
    "javascript"
  ],
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": false,
      "source.fixAll.eslint": true,
      "source.fixAll.stylelint": false
    }
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": false,
      "source.fixAll.eslint": true,
      "source.fixAll.stylelint": false
    }
  }
}